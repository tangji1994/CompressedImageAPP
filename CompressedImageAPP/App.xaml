<Application x:Class="CompressedImageAPP.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:CompressedImageAPP"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        
        <ResourceDictionary>

            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resource/ModernTheme.xaml"/>
                <ResourceDictionary Source="Resource/AppIcon.xaml"/>
                <ResourceDictionary Source="Resource/Style/Button.xaml"/>
                <ResourceDictionary Source="Resource/Style/ListView.xaml"/>
                <ResourceDictionary Source="Resource/Style/Slider.xaml"/>
                <ResourceDictionary Source="Resource/Style/ComboBox.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
